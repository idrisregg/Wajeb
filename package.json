{
  "name": "wajeb-app",
  "version": "1.0.0",
  "description": "Wajeb File Management Application",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd back && npm run dev",
    "dev:frontend": "cd front && npm run dev",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd back && npm start",
    "start:frontend": "cd front && npm run build && npm run preview",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd front && npm run build",
    "build:backend": "cd back && npm run build",
    "install:all": "npm install && cd back && npm install && cd ../front && npm install",
    "clean": "rm -rf node_modules back/node_modules front/node_modules",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd back && npm test",
    "test:frontend": "cd front && npm test"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@fastify/multipart": "^9.2.1"
  }
}
